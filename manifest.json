{
    "manifest_version": 2,
    "name": "imdb movie rating",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "background": {
        "scripts": [
            "jquery-2.2.2.min.js",
            "xhr.js",
            "main.js"
        ]
    },

    "permissions": [
        "tabs",
        "bookmarks",
        "http://*/**",
        "https://*/**",
        "unlimitedStorage"
    ]
}
